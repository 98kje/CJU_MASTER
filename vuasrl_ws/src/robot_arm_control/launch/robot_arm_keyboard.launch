<launch>
  <arg name="robot_name"   default="tugcar"/>
  <arg name="end_effector" default="gripper"/>

  <node name="teleop_keyboard" pkg="robot_arm_control" type="controller" output="screen">
    <remap from="kinematics_pose" to="$(arg end_effector)/kinematics_pose"/>
    <param name="end_effector_name" value="$(arg end_effector)"/>
  </node>
</launch>

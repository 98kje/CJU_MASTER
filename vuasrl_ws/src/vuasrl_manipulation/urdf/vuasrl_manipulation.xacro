<?xml version="1.0" ?>
<robot name="vuasrl_manipulation" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find vuasrl_manipulation)/urdf/vuasrl_manipulation.gazebo.xacro"/>

  <!-- Import Transmission -->
  <xacro:include filename="$(find vuasrl_manipulation)/urdf/vuasrl_manipulation.transmission.xacro"/>


  <link name="link1">
    <inertial>
      <origin xyz="0.0001816853136070254 -2.1915878376977466e-17 0.020129887613482035" rpy="0 0 0"/>
      <mass value="7.9119962e-02"/>
      <inertia ixx="0.002145" iyy="0.004951" izz="0.006213" ixy="-0.0" iyz="-0.0" ixz="-1.3e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="link2">
    <inertial>
      <origin xyz="-2.328586629074536e-11 -2.8877387782838815e-10 0.020681716416087625" rpy="0 0 0"/>
      <mass value="9.8406837e-02"/>
      <inertia ixx="0.000355" iyy="0.00039" izz="0.000302" ixy="0.0" iyz="-0.0" ixz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.03 -0.0 -0.048" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.03 -0.0 -0.048" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="link3">
    <inertial>
      <origin xyz="-0.004542317857195898 0.0005782036806908492 0.09874905854464662" rpy="0 0 0"/>
      <mass value="1.3850917e-01"/>
      <inertia ixx="0.009439" iyy="0.009407" izz="0.0009" ixy="-1e-06" iyz="1.5e-05" ixz="0.000653"/>
    </inertial>
    <visual>
      <origin xyz="-0.03 -0.0 -0.078" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.03 -0.0 -0.078" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="link4">
    <inertial>
      <origin xyz="4.336808689942018e-18 0.0005787515631340847 0.10231346303330496" rpy="0 0 0"/>
      <mass value="1.3274562e-01"/>
      <inertia ixx="0.010801" iyy="0.010515" izz="0.000691" ixy="0.0" iyz="1.6e-05" ixz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.005 -0.0005 -0.243" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.005 -0.0005 -0.243" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link4.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="link5">
    <inertial>
      <origin xyz="2.8536201179818477e-16 -5.72784007724092e-15 0.04451102048440414" rpy="0 0 0"/>
      <mass value="1.4327573e-01"/>
      <inertia ixx="0.00218" iyy="0.00154" izz="0.000828" ixy="-0.0" iyz="0.0" ixz="-0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.005 -0.001 -0.433" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.005 -0.001 -0.433" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="gripper">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0e-03"/>
      <inertia ixx="1.0e-03" iyy="1.0e-03" izz="1.0e-03" ixy="0.0" iyz="0.0" ixz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.01 0.033 -0.95715" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/grip.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.01 0.033 -0.95715" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/grip.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="gripper_sub">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.0e-03"/>
      <inertia ixx="1.0e-03" iyy="1.0e-03" izz="1.0e-03" ixy="0.0" iyz="0.0" ixz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="-0.01 -0.037 -0.95715" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/grip_sub.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="-0.01 -0.037 -0.95715" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://vuasrl_manipulation/meshes/grip_sub.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint1" type="revolute">
    <origin xyz="0.03 0.0 0.048" rpy="0 0 0"/>
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*0.9}" upper="${pi*0.9}" />
  </joint>

  <joint name="joint2" type="revolute">
    <origin xyz="0.0 0.0 0.03" rpy="0 0 0"/>
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="0.0 1.0 0.0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*0.57}" upper="${pi*0.5}" />
  </joint>

  <joint name="joint3" type="revolute">
    <origin xyz="-0.025 0.0005 0.165" rpy="0 0 0"/>
    <parent link="link3"/>
    <child link="link4"/>
    <axis xyz="0.0 1.0 0.0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*0.3}" upper="${pi*0.44}" />
  </joint>

  <joint name="joint4" type="revolute">
    <origin xyz="0.0 0.0005 0.19" rpy="0 0 0"/>
    <parent link="link4"/>
    <child link="link5"/>
    <axis xyz="0.0 1.0 0.0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*0.57}" upper="${pi*0.65}" />
  </joint>

  <joint name="grip_joint" type="prismatic">
    <origin xyz="0.005 -0.034 0.52415" rpy="0 0 0"/>
    <parent link="link5"/>
    <child link="gripper"/>
    <axis xyz="0.0 1.0 0.0"/>
    <limit velocity="4.8" effort="1" lower="-0.010" upper="0.019" />
  </joint>

  <joint name="grip_joint_sub" type="prismatic">
    <origin xyz="0.005 0.036 0.52415" rpy="0 0 0"/>
    <parent link="link5"/>
    <child link="gripper_sub"/>
    <axis xyz="0.0 -1.0 0.0"/>
    <limit velocity="4.8" effort="1" lower="-0.010" upper="0.019" />
  <!--  <mimic joint="grip_joint" multiplier="1"/> -->
  </joint>

  <!-- end effector joint -->
  <joint name="end_effector_joint" type="fixed">
    <origin xyz="0.0 0.0 0.115" rpy="0 0 0"/>
    <parent link="link5"/>
    <child link="end_effector_link"/>
  </joint>

  <!-- end effector link -->
  <link name="end_effector_link">
      <visual>
          <origin rpy="0 0 0" xyz="0 0 0" />
          <geometry>
              <box size="0.01 0.01 0.01" />
          </geometry>
          <material name="red" />
      </visual>
      <collision>
          <origin rpy="0 0 0" xyz="0 0 0" />
          <geometry>
              <box size="0.01 0.01 0.01" />
          </geometry>
      </collision>
      <inertial>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <mass value="1.0e-03" />
          <inertia ixx="1.0e-03" ixy="0.0" ixz="0.0" iyy="1.0e-03" iyz="0.0" izz="1.0e-03" />
      </inertial>
  </link>

</robot>
